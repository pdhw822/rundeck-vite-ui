{
  "name": "@rundeck/ui-trellis-vite",
  "version": "4.0",
  "description": "A collection of utilities used across various javascript applications and components.",
  "author": "Miguel Ramos <mramos@pagerduty.com>",
  "license": "ISC",
  "main": "./lib/uitrellis.umd.js",
  "module": "./lib/uitrellis.es.js",
  "exports": {
    ".": {
      "import": "./lib/uitrellis.es.js",
      "require": "./lib/uitrellis.umd.js"
    },
    "./lib/style.css": "./lib/style.css"
  },
  "bugs": {
    "url": "https://github.com/rundeck/ui-trellis/issues"
  },
  "homepage": "https://github.com/rundeck/ui-trellis#readme",
  "scripts": {
    "dev:app": "vite -c vite.config.app.js --mode development",
    "devbuild:app": "vite build -c vite.config.app.js --mode development --watch",
    "watch:devbuild:app": "vite build -c vite.config.app.js --mode development --watch",
    "watchbuild:app": "vite build -c vite.config.app.js --mode production --watch",
    "build:app": "vite build -c vite.config.app.js --mode production",
    "build:library": "vite build -c vite.config.library.js --mode production",
    "dev:build": "npm install && npm run build:library",
    "dev:build:app": "npm install && npm run build:app",
    "ci:build": "npm ci && npm run build:library",
    "ci:build:app": "npm ci && npm run build:app"
  },
  "dependencies": {
    "@iconfu/svg-inject": "^1.2.3",
    "@popperjs/core": "^2.8.4",
    "@rundeck/client": "^0.2.5",
    "@vue/composition-api": "^1.6.1",
    "@vueuse/core": "^8.5.0",
    "ace-builds": "^1.4.12",
    "core-js": "^3.21.1",
    "deep-object-diff": "^1.1.7",
    "i": "^0.3.7",
    "lodash": "^4.17.21",
    "markdown-it-vue": "^1.1.7",
    "moment": "^2.25.3",
    "npm": "^8.18.0",
    "perfect-scrollbar": "^1.5.0",
    "prex": "^0.4.7",
    "uiv": "^0.33.0",
    "vue": "^2.6.14",
    "vue-fuse": "^2.2.1",
    "vue-good-table": "^2.21.11",
    "vue-i18n": "^8.22.4",
    "vue-moment": "^4.0.0",
    "vue-multiselect": "^2.1.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.0.6",
    "vue-scrollto": "^2.17.1",
    "vue-virtual-scroller": "^1.0.10",
    "vue2-filters": "^0.9.1",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.14",
    "@types/lodash": "^4.14.183",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/runtime-dom": "^3.2.37",
    "mobx-utils": "^5.6.1",
    "mobx-vue": "^2.0.11",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.52.1",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.4",
    "unplugin-icons": "^0.13.3",
    "unplugin-vue-components": "^0.18.0",
    "unplugin-vue2-script-setup": "^0.10.0",
    "vite": "^2.9.14",
    "vite-plugin-externals": "^0.5.1",
    "vite-plugin-vue2": "^1.9.3",
    "vite-plugin-windicss": "^1.8.3",
    "vue-cookies": "^1.8.1",
    "vue-template-compiler": "^2.6.14"
  }
}
